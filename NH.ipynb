   "source": [
    "N = 2500\n",
    "beta = 0.8\n",
    "gamma = 0.1\n",
    "limit = 0.8\n",
    "\n",
    "matrix = np.zeros([N,N], dtype=int)     #adjacency matrix\n",
    "state = np.ones(N, dtype=int)\n",
    "\n",
    "i = 0\n",
    "while i < 2*N:\n",
    "    place = np.random.randint(0,N, size=2)\n",
    "    if matrix[place[0], place[1]] == 1 or place[0] == place[1]:\n",
    "        continue\n",
    "    else:\n",
    "        matrix[place[0], place[1]] = 1   #connections with the same wight\n",
    "        matrix[place[1], place[0]] = 1  #symmetric matrix\n",
    "        i += 1\n",
    "\n",
    "place = np.random.randint(N)\n",
    "state[place] = 2   #infected\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "# print(matrix)\n",
    "# print(state)\n",
    "\n",
    "sus, inf, rec, t, case = iterate_random(matrix, state, beta, gamma, limit)\n",
    "\n",
    "plt.plot(range(t+1), sus, range(t+1), inf, range(t+1), rec)\n",
    "plt.legend(['sus', 'inf', 'rec'])\n",
    "plt.show()"
   ]
  },
  {
